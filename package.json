{
  "name": "servertest",
  "module": "server.ts",
  "type": "module",
  "scripts": {
    "migrate": "bun -b kysely-migrate",
    "db:start": "docker-compose up -d",
    "db:stop": "docker-compose down",
    "start": "docker-compose up -d && bun --watch run src/server.ts",
    "build": "bun build ./src/**/*.ts ./src/server.ts --outdir ./build"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/express": "^4.17.21",
    "kysely-migrate": "^0.0.16"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "express": "^4.19.2",
    "kysely": "^0.27.3",
    "mysql2": "^3.9.4"
  }
}